{
  "name": "@effect-ts/morphic",
  "version": "0.8.15",
  "license": "MIT",
  "homepage": "https://github.com/Matechs-Garage/matechs-effect",
  "private": false,
  "scripts": {
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "tsc --build tsconfig.build.json && ts-node ../../scripts/package.ts",
    "build:es6": "tsc --build tsconfig.es6.json && ts-node ../../scripts/rewrite-esm.ts && babel build/esm/ -d build/esm/ -s --plugins annotate-pure-calls",
    "build:post-process": "ts-node ../../scripts/rewrite-source-maps.ts && ts-node ../../scripts/rewrite-locals.ts",
    "build": "yarn run build:lib && yarn run build:es6 && yarn run build:post-process",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "tc": "tsc --noEmit",
    "demo": "ts-node -r tsconfig-paths/register"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "repository": "https://github.com/Matechs-Garage/matechs-effect.git",
  "sideEffects": false,
  "dependencies": {
    "@effect-ts/core": "^0.8.5",
    "@effect-ts/monocle": "^0.7.5"
  },
  "config": {
    "modules": [
      "Adt",
      "Adt/ctors",
      "Adt/matcher",
      "Adt/monocle",
      "Adt/predicates",
      "Adt/utils",
      "Algebra",
      "Algebra/Config",
      "Algebra/Core",
      "Algebra/Intersection",
      "Algebra/Newtype",
      "Algebra/Object",
      "Algebra/Primitives",
      "Algebra/Record",
      "Algebra/Recursive",
      "Algebra/Refined",
      "Algebra/Set",
      "Algebra/TaggedUnion",
      "Algebra/Union",
      "Algebra/Unknown",
      "Asserts",
      "Batteries",
      "Batteries/interpreter",
      "Batteries/program",
      "Batteries/summoner",
      "Batteries/usage",
      "Batteries/usage/interpreter-result",
      "Batteries/usage/materializer",
      "Batteries/usage/program-infer",
      "Batteries/usage/program-type",
      "Batteries/usage/summoner",
      "Batteries/usage/tagged-union",
      "Batteries/usage/utils",
      "Decoder",
      "Decoder/base",
      "Decoder/common",
      "Decoder/interpreter",
      "Decoder/reporters",
      "Encoder",
      "Encoder/base",
      "Encoder/interpreter",
      "Equal",
      "Equal/base",
      "Equal/interpreter",
      "FastCheck",
      "FastCheck/base",
      "FastCheck/interpreter",
      "Guard",
      "Guard/base",
      "Guard/interpreter",
      "Hash",
      "Hash/base",
      "Hash/interpreter",
      "HKT",
      "Show",
      "Show/base",
      "Show/interpreter",
      "Strict",
      "Strict/base",
      "Strict/interpreter",
      "StrictDecoder",
      "Utils"
    ]
  },
  "peerDependencies": {
    "fast-check": "^2.4.0",
    "fast-equals": "^2.0.0"
  }
}
