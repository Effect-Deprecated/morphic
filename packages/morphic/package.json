{
  "name": "@effect-ts/morphic",
  "version": "0.39.0",
  "license": "MIT",
  "repository": "https://github.com/Effect-TS/morphic.git",
  "homepage": "https://www.matechs.com",
  "scripts": {
    "clean": "yarn ets:rimraf build tsbuildinfo dist",
    "build-cjs": "yarn ets:tsc -p tsconfig.build.cjs.json",
    "build-esm": "yarn ets:tsc -p tsconfig.build.esm.json",
    "build-post": "yarn ets:build-utils pack",
    "build": "yarn ets:concurrently \"yarn build-cjs\" \"yarn build-esm\" && yarn build-post",
    "lint": "yarn ets:eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "yarn ets:prettier --write \"./{src,test,demo}/**/*.ts\"",
    "tc": "yarn ets:tsc --noEmit",
    "circular": "yarn ets:madge --circular --ts-config ./tsconfig.json --extensions ts ./src"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "config": {
    "side": [],
    "modules": [
      "Adt",
      "Adt/ctors",
      "Adt/matcher",
      "Adt/monocle",
      "Adt/predicates",
      "Adt/utils",
      "Algebra",
      "Algebra/Config",
      "Algebra/Core",
      "Algebra/Intersection",
      "Algebra/Newtype",
      "Algebra/Object",
      "Algebra/Primitives",
      "Algebra/Record",
      "Algebra/Recursive",
      "Algebra/Refined",
      "Algebra/Set",
      "Algebra/TaggedUnion",
      "Algebra/Union",
      "Algebra/Unknown",
      "Asserts",
      "Batteries",
      "Batteries/interpreter",
      "Batteries/program",
      "Batteries/summoner",
      "Batteries/usage",
      "Batteries/usage/interpreter-result",
      "Batteries/usage/materializer",
      "Batteries/usage/program-infer",
      "Batteries/usage/program-type",
      "Batteries/usage/summoner",
      "Batteries/usage/tagged-union",
      "Batteries/usage/utils",
      "Decoder",
      "Decoder/base",
      "Decoder/common",
      "Decoder/interpreter",
      "Decoder/reporters",
      "Encoder",
      "Encoder/base",
      "Encoder/interpreter",
      "Equal",
      "Equal/base",
      "Equal/interpreter",
      "FastCheck",
      "FastCheck/base",
      "FastCheck/interpreter",
      "Guard",
      "Guard/base",
      "Guard/interpreter",
      "TypeHash",
      "TypeHash/base",
      "TypeHash/interpreter",
      "HKT",
      "Reorder",
      "Reorder/base",
      "Reorder/interpreter",
      "Show",
      "Show/base",
      "Show/interpreter",
      "Strict",
      "Strict/base",
      "Strict/interpreter",
      "StrictDecoder",
      "Utils"
    ]
  },
  "peerDependencies": {
    "@effect-ts/core": "^0.53.0",
    "@effect-ts/monocle": "^0.40.1",
    "fast-check": ">= 2.14.0"
  }
}
