{
  "name": "@effect-ts/morphic",
  "version": "0.31.0",
  "license": "MIT",
  "repository": "https://github.com/Effect-TS/morphic.git",
  "homepage": "https://www.matechs.com",
  "private": false,
  "scripts": {
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "ttsc --build  tsconfig.build.json && build-utils package",
    "build:es6": "ttsc --build  tsconfig.es6.json && babel build/esm/ -d build/esm/ -s --plugins annotate-pure-calls && build-utils package-esm",
    "build:post-process": "build-utils rewrite-source-maps && build-utils rewrite-locals && yarn prettier:lib",
    "build": "yarn run build:lib && yarn run build:es6 && yarn run build:post-process",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "prettier:lib": "prettier --write \"./build/**/*.js\"",
    "tc": "tsc --noEmit",
    "demo": "ts-node -r tsconfig-paths/register"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "sideEffects": false,
  "config": {
    "modules": [
      "Adt",
      "Adt/ctors",
      "Adt/matcher",
      "Adt/monocle",
      "Adt/predicates",
      "Adt/utils",
      "Algebra",
      "Algebra/Config",
      "Algebra/Core",
      "Algebra/Intersection",
      "Algebra/Newtype",
      "Algebra/Object",
      "Algebra/Primitives",
      "Algebra/Record",
      "Algebra/Recursive",
      "Algebra/Refined",
      "Algebra/Set",
      "Algebra/TaggedUnion",
      "Algebra/Union",
      "Algebra/Unknown",
      "Asserts",
      "Batteries",
      "Batteries/interpreter",
      "Batteries/program",
      "Batteries/summoner",
      "Batteries/usage",
      "Batteries/usage/interpreter-result",
      "Batteries/usage/materializer",
      "Batteries/usage/program-infer",
      "Batteries/usage/program-type",
      "Batteries/usage/summoner",
      "Batteries/usage/tagged-union",
      "Batteries/usage/utils",
      "Decoder",
      "Decoder/base",
      "Decoder/common",
      "Decoder/interpreter",
      "Decoder/reporters",
      "Encoder",
      "Encoder/base",
      "Encoder/interpreter",
      "Equal",
      "Equal/base",
      "Equal/interpreter",
      "FastCheck",
      "FastCheck/base",
      "FastCheck/interpreter",
      "Guard",
      "Guard/base",
      "Guard/interpreter",
      "TypeHash",
      "TypeHash/base",
      "TypeHash/interpreter",
      "HKT",
      "Reorder",
      "Reorder/base",
      "Reorder/interpreter",
      "Show",
      "Show/base",
      "Show/interpreter",
      "Strict",
      "Strict/base",
      "Strict/interpreter",
      "StrictDecoder",
      "Utils"
    ]
  },
  "peerDependencies": {
    "@effect-ts/core": "^0.32.0",
    "@effect-ts/monocle": "^0.29.0",
    "fast-check": ">= 2.14.0"
  }
}
